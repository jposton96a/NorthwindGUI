<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.layout.ColumnConstraints?>

<BorderPane prefHeight="400.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.ListSupplierView">
   <top>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Supplier List:" BorderPane.alignment="TOP_LEFT">
         <font>
            <Font name="Segoe UI Light" size="25.0" />
         </font>
         <BorderPane.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </BorderPane.margin></Text>
   </top>
   <center>
      <ScrollPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <content>
            <GridPane fx:id="supplierList" prefHeight="230.0" prefWidth="581.0">
                <columnConstraints>
                    <ColumnConstraints minWidth="20.0" />
                </columnConstraints>
            </GridPane>
         </content></ScrollPane>
   </center>
   <bottom>
       <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="560.0" BorderPane.alignment="CENTER">
           <children>
               <Button fx:id="btnBack" mnemonicParsing="false" text="Go Back" />
           </children>
           <opaqueInsets>
               <Insets />
           </opaqueInsets>
           <BorderPane.margin>
               <Insets left="20.0" right="20.0" />
           </BorderPane.margin>
       </HBox>
   </bottom>
</BorderPane>
